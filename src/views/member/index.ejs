    <script>
        // Function to copy text to clipboard
        function copyToClipboard(text) {
            const tempInput = document.createElement('input');
            tempInput.value = text;
            document.body.appendChild(tempInput);
            tempInput.select();
            document.execCommand('copy');
            document.body.removeChild(tempInput);
        }

        document.addEventListener('DOMContentLoaded', function() {
            // Fetch user information
            fetch('/api/webapi/GetUserInfo')
                .then(response => response.json())
                .then(data => {
                    if (data.status === false) {
                        unsetCookie();
                        return false;
                    }
                    document.querySelector('.infoName .name').textContent = data.data.name_user;
                    document.querySelector('.infoName .id').innerHTML = `UID&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;|&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;${data.data.id_user} &nbsp;&nbsp;&nbsp;<img data-v-0341cbde="" style="width: 13px; margin-bottom: -1px;" src="/assets/png/copy-3a78d902.png">`;
                    document.querySelector('.infoName .id').setAttribute('data-clipboard-text', data.data.id_user);
                    document.querySelector('.infoName .number').textContent = `Mobile : +91 ${data.data.phone_user.slice(0, 2)}****${data.data.phone_user.slice(-4)}`;
                    document.querySelector('.money .txt').textContent = ` ₹ ${data.data.money_user} `;



                    // Create the image element
                    var imgElement = document.createElement('img');
                    imgElement.setAttribute('data-v-7d9f9f69', '');
                    imgElement.setAttribute('src', '/assets/png/0-78e1ab02.png');

                    // Add styles to the image element
                    imgElement.style.marginLeft = '0.2rem'; // Adjust the margin as needed
                    imgElement.style.width = '1.3rem'; // Adjust the width as needed



                    // Append the image next to the user's name
                    var nameElement = document.querySelector('.infoName .name');
                    nameElement.appendChild(imgElement);

                });

            // Add event listener to the copy button
            document.querySelector('.copyButton').addEventListener('click', function() {
                const dataText = document.querySelector('.infoName .id').getAttribute('data-clipboard-text');
                copyToClipboard(dataText);
                alert('Copied: ' + dataText);
            });
        });
    </script>
